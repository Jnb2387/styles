{
  "version": 8,
  "name": "DHA Style",
  "metadata": {"maputnik:renderer": "mbgljs"},
  "sources": {
    "tilehut": {
      "type": "vector",
      "tiles": ["http://px-geo02-d:3001/colorado/{z}/{x}/{y}.pbf"],
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "sprite": "http://px-geo02-d:3001/sprite_permit",
  "glyphs": "https://orangemug.github.io/font-glyphs/glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {"background-color": "rgba(249, 246, 246, 1)"}
    },
    {
      "id": "landuse-residential",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landuse",
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": [
          "interpolate",
          ["linear"],
          ["zoom"],
          15,
          [
            "match",
            ["get", "class"],
            "park",
            "#b6e59e",
            "airport",
            "hsl(230, 15%, 86%)",
            "cemetery",
            "hsl(75, 37%, 81%)",
            "glacier",
            "hsl(196, 72%, 93%)",
            "hospital",
            "hsl(340, 37%, 87%)",
            "pitch",
            "hsl(100, 57%, 72%)",
            "sand",
            "hsl(60, 46%, 87%)",
            "school",
            "hsl(50, 47%, 81%)",
            "hsla(0, 0%, 0%, 0)"
          ],
          16,
          [
            "match",
            ["get", "class"],
            "park",
            "hsl(100, 58%, 76%)",
            "airport",
            "hsl(230, 29%, 89%)",
            "cemetery",
            "hsl(75, 37%, 81%)",
            "glacier",
            "hsl(196, 72%, 93%)",
            "hospital",
            "hsl(340, 63%, 89%)",
            "pitch",
            "hsl(100, 57%, 72%)",
            "sand",
            "hsl(60, 46%, 87%)",
            "school",
            "hsl(50, 63%, 84%)",
            "hsla(0, 0%, 0%, 0)"
          ]
        ],
        "fill-opacity": 0.7
      }
    },
    {
      "id": "landcover_grass",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landcover",
      "filter": ["==", "class", "grass"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "hsl(82, 46%, 72%)", "fill-opacity": 0.8}
    },
    {
      "id": "landcover_wood",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landcover",
      "filter": ["==", "class", "wood"],
      "paint": {
        "fill-color": "hsl(82, 46%, 72%)",
        "fill-opacity": {"base": 1, "stops": [[8, 0.6], [22, 1]]}
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "water",
      "filter": ["==", "$type", "Polygon"],
      "paint": {"fill-color": "hsl(205, 56%, 73%)"}
    },
    {
      "id": "landcover-ice-shelf",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landcover",
      "filter": ["==", "subclass", "ice_shelf"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "hsl(47, 26%, 88%)", "fill-opacity": 0.8}
    },
    {
      "id": "landcover-glacier",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landcover",
      "filter": ["==", "subclass", "glacier"],
      "layout": {"visibility": "visible"},
      "paint": {
        "fill-color": "hsl(47, 22%, 94%)",
        "fill-opacity": {"base": 1, "stops": [[0, 1], [8, 0.5]]}
      }
    },
    {
      "id": "landcover_sand",
      "type": "fill",
      "metadata": {},
      "source": "tilehut",
      "source-layer": "landcover",
      "filter": ["all", ["in", "class", "sand"]],
      "paint": {
        "fill-antialias": false,
        "fill-color": "rgba(232, 214, 38, 1)",
        "fill-opacity": 0.3
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landuse",
      "filter": ["==", "class", "agriculture"],
      "layout": {"visibility": "visible"},
      "paint": {"fill-color": "#eae0d0"}
    },
    {
      "id": "landuse_overlay_national_park",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "landcover",
      "filter": ["==", "class", "national_park"],
      "paint": {
        "fill-color": "#E1EBB0",
        "fill-opacity": {"base": 1, "stops": [[5, 0], [9, 0.75]]}
      }
    },
    {
      "id": "waterway-tunnel",
      "type": "line",
      "source": "tilehut",
      "source-layer": "waterway",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "brunnel", "tunnel"]
      ],
      "paint": {
        "line-color": "hsl(205, 56%, 73%)",
        "line-dasharray": [3, 3],
        "line-gap-width": {"stops": [[12, 0], [20, 6]]},
        "line-opacity": 1,
        "line-width": {"base": 1.4, "stops": [[8, 1], [20, 2]]}
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "tilehut",
      "source-layer": "waterway",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["!in", "brunnel", "tunnel", "bridge"]
      ],
      "paint": {
        "line-color": "hsl(205, 56%, 73%)",
        "line-opacity": 1,
        "line-width": {"base": 1.4, "stops": [[8, 1], [20, 8]]}
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "tilehut",
      "source-layer": "building",
      "paint": {
        "fill-antialias": true,
        "fill-color": "rgba(222, 211, 190, 1)",
        "fill-opacity": {"base": 1, "stops": [[13, 0], [15, 1]]},
        "fill-outline-color": {
          "stops": [
            [15, "rgba(212, 177, 146, 0)"],
            [16, "rgba(212, 177, 146, 0.5)"]
          ]
        }
      }
    },
    {
      "id": "road_path",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "path", "track"]
      ],
      "layout": {"line-cap": "square", "line-join": "bevel"},
      "paint": {
        "line-color": "rgba(24, 1, 1, 1)",
        "line-dasharray": [1, 1],
        "line-width": {"base": 1.55, "stops": [[4, 0.25], [20, 10]]}
      }
    },
    {
      "id": "road_minor",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "minzoom": 13,
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "minor", "service"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "rgba(158, 126, 126, 1)",
        "line-width": {"base": 1.55, "stops": [[4, 0.25], [20, 30]]}
      }
    },
    {
      "id": "road_trunk_primary",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "trunk", "primary"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "rgba(201, 193, 193, 1)",
        "line-width": {"base": 1.4, "stops": [[6, 0.5], [20, 30]]}
      }
    },
    {
      "id": "road_secondary_tertiary",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["in", "class", "secondary", "tertiary"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "rgba(211, 200, 200, 1)",
        "line-width": {"base": 1.4, "stops": [[6, 0.5], [20, 20]]}
      }
    },
    {
      "id": "road_major_motorway",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "class", "motorway"],
        ["!=", "ramp", 1]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "rgba(205, 69, 69, 1)",
        "line-offset": 0,
        "line-width": {"base": 1.55, "stops": [[4, 2.25], [20, 30]]}
      }
    },
    {
      "id": "railway-transit",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "filter": [
        "all",
        ["==", "class", "transit"],
        ["!=", "brunnel", "tunnel"]
      ],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "hsl(34, 12%, 66%)",
        "line-opacity": {"base": 1, "stops": [[11, 0], [16, 1]]}
      }
    },
    {
      "id": "railway",
      "type": "line",
      "source": "tilehut",
      "source-layer": "transportation",
      "filter": ["==", "class", "rail"],
      "layout": {"visibility": "visible"},
      "paint": {
        "line-color": "hsl(34, 12%, 66%)",
        "line-opacity": {"base": 1, "stops": [[11, 0], [16, 1]]}
      }
    },
    {
      "id": "waterway-bridge-case",
      "type": "line",
      "source": "tilehut",
      "source-layer": "waterway",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "brunnel", "bridge"]
      ],
      "layout": {"line-cap": "butt", "line-join": "miter"},
      "paint": {
        "line-color": "#bbbbbb",
        "line-gap-width": {"base": 1.55, "stops": [[4, 0.25], [20, 30]]},
        "line-width": {"base": 1.6, "stops": [[12, 0.5], [20, 10]]}
      }
    },
    {
      "id": "waterway-bridge",
      "type": "line",
      "source": "tilehut",
      "source-layer": "waterway",
      "filter": [
        "all",
        ["==", "$type", "LineString"],
        ["==", "brunnel", "bridge"]
      ],
      "layout": {"line-cap": "round", "line-join": "round"},
      "paint": {
        "line-color": "hsl(205, 56%, 73%)",
        "line-width": {"base": 1.55, "stops": [[4, 0.25], [20, 30]]}
      }
    },
    {
      "id": "place_label_other",
      "type": "symbol",
      "source": "tilehut",
      "source-layer": "place",
      "minzoom": 8,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["!in", "class", "city", "state", "country", "continent"]
      ],
      "layout": {
        "text-anchor": "center",
        "text-field": "{name:latin}\n{name:nonlatin}",
        "text-font": ["Noto Sans Regular"],
        "text-max-width": 6,
        "text-size": {"stops": [[6, 6], [12, 10]]},
        "visibility": "visible"
      },
      "paint": {
        "text-color": "hsl(0, 0%, 25%)",
        "text-halo-blur": 0,
        "text-halo-color": "hsl(0, 0%, 100%)",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "country_label-other",
      "type": "symbol",
      "source": "tilehut",
      "source-layer": "place",
      "maxzoom": 12,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "country"],
        ["!has", "iso_a2"]
      ],
      "layout": {
        "text-field": "{name:latin}",
        "text-font": ["Noto Sans Regular"],
        "text-max-width": 10,
        "text-size": {"stops": [[3, 12], [8, 22]]},
        "visibility": "visible"
      },
      "paint": {
        "text-color": "hsl(0, 0%, 13%)",
        "text-halo-blur": 0,
        "text-halo-color": "rgba(255,255,255,0.75)",
        "text-halo-width": 2
      }
    },
    {
      "id": "country_label",
      "type": "symbol",
      "source": "tilehut",
      "source-layer": "place",
      "maxzoom": 12,
      "filter": [
        "all",
        ["==", "$type", "Point"],
        ["==", "class", "country"],
        ["has", "iso_a2"]
      ],
      "layout": {
        "text-field": "{name:latin}",
        "text-font": ["Noto Sans Bold"],
        "text-max-width": 10,
        "text-size": {"stops": [[3, 12], [8, 22]]},
        "visibility": "visible"
      },
      "paint": {
        "text-color": "hsl(0, 0%, 13%)",
        "text-halo-blur": 0,
        "text-halo-color": "rgba(255,255,255,0.75)",
        "text-halo-width": 2
      }
    },
    {
      "id": "place_label_city",
      "type": "symbol",
      "source": "tilehut",
      "source-layer": "place",
      "maxzoom": 16,
      "filter": ["all", ["==", "$type", "Point"], ["==", "class", "city"]],
      "layout": {
        "text-field": "{name:latin}\n{name:nonlatin}",
        "text-font": ["Noto Sans Regular"],
        "text-allow-overlap": true,
        "text-ignore-placement": true,
        "text-max-width": 10,
        "text-size": [
          "interpolate",
          ["cubic-bezier", 0.2, 0, 0.9, 1],
          ["zoom"],
          3,
          [
            "step",
            ["get", "rank"],
            13,
            3,
            12,
            7,
            11,
            10,
            10.5,
            12,
            9.5,
            14,
            8.5,
            16,
            6.5,
            17,
            4
          ],
          16,
          [
            "step",
            ["get", "rank"],
            30,
            3,
            28,
            7,
            26,
            10,
            23,
            11,
            21,
            12,
            20,
            13,
            19,
            15,
            17
          ]
        ],
        "visibility": "visible"
      },
      "paint": {
        "text-color": "hsl(0, 0%, 0%)",
        "text-halo-blur": 0,
        "text-halo-color": "hsla(0, 0%, 100%, 0.75)",
        "text-halo-width": 2
      }
    }
  ],
  "symbol-z-order": "source",
  "id": "klokantech-basic",
  "owner": "Jeffrey"
}
